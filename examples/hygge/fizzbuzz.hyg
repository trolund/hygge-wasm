fun fb(i: int): int = {
    let by3: bool = ((i % 3) = 0);
    let by5: bool = ((i % 5) = 0);

    if (by3) then {
        if (by5) then {
            10
        } 
        else {
            12
        }
    } 
    else {
        if (by5) then {
            13
        } 
        else {
            14
        }
    }
};

let mutable x: int = 0;
let mutable y: int = 10;

for ((); (x < y); {x <- x + 1})
    println(fb(x))