let mutable i: int = 0;
let mutable y: int = 12;

for ((); (i < y); {i <- i + 1}) {
    
    let by3: bool = ((i % 3) = 0);
    let by5: bool = ((i % 5) = 0);

    if (by3) then {
        if (by5) then {
            println("FizzBuzz")
        } 
        else {
            println("Fizz")
        }
    } 
    else {
        if (by5) then {
            println("Buzz")
        } 
        else {
            println(i)
        }
    }
}