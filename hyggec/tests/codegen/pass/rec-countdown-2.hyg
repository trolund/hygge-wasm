let countdown: (int) -> int = fun(start: int) -> {
    let x: int = 100;
    let rec countdownRec: (int) -> int = fun(curr: int) -> {
        if (curr < 0) then {
            println("Go!");
            curr + x // -1 + x
        } 
        else {
            println("Countdown: ");
            println(curr + x);
            countdownRec(curr - 1)
        }
    };

    countdownRec(start)
};

assert(countdown(10) = 99)