let n: int = 10;
let temp: array {int} = array(2, 0);
let col: array {array {int}} = array(n, temp);

let mutable i: int = 0;
while (i < arrayLength(col)) do {
    let row: array {int} = array(n, 42 + i);
    print(i);
    print(arrayLength(row));
    arrayElem(col, i) <- row;
    i <- i + 1
};

i <- 0;
while (i < arrayLength(col)) do {
    let outter: array {int} = arrayElem(col, i);
    let res: int = arrayElem(outter, 0);
    print(res);
    assert(res = 42 + i);
    i <- i + 1
};

print("Done!")